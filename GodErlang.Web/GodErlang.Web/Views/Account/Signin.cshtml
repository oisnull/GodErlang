
@{
    Layout = null;
    ViewData["Title"] = "GodErlang Management Sign in";
}

<!DOCTYPE html>
<!-- saved from url=(0057)http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html -->
<html class="login-bg" style="background-image: url(&quot;img/bgs/11.jpg&quot;);">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>GodErlang Management Sign in</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- bootstrap -->
    <link href="./Detail Admin - Sign in_files/bootstrap.css" rel="stylesheet">
    <link href="./Detail Admin - Sign in_files/bootstrap-responsive.css" rel="stylesheet">
    <link href="./Detail Admin - Sign in_files/bootstrap-overrides.css" type="text/css" rel="stylesheet">

    <!-- global styles -->
    <link rel="stylesheet" type="text/css" href="./Detail Admin - Sign in_files/layout.css">
    <link rel="stylesheet" type="text/css" href="./Detail Admin - Sign in_files/elements.css">
    <link rel="stylesheet" type="text/css" href="./Detail Admin - Sign in_files/icons.css">

    <!-- libraries -->
    <link rel="stylesheet" type="text/css" href="./Detail Admin - Sign in_files/font-awesome.css">

    <!-- this page specific styles -->
    <link rel="stylesheet" href="./Detail Admin - Sign in_files/signin.css" type="text/css" media="screen">

    <!-- open sans font -->
    <link href="http://fonts.useso.com/css?family=Open+Sans:300italic,400italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css">

    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <script src="chrome-extension://mooikfkahbdckldjjndioackbalphokd/assets/prompt.js"></script>
</head>
<body>


    <!-- background switcher -->
    <div class="bg-switch visible-desktop">
        <div class="bgs">
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="landscape.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/landscape.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="blueish.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/blueish.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="7.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/7.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="8.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/8.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="9.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/9.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="10.jpg" class="bg">
                <img src="./Detail Admin - Sign in_files/10.jpg">
            </a>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" data-img="11.jpg" class="bg active">
                <img src="./Detail Admin - Sign in_files/11.jpg">
            </a>
        </div>
    </div>


    <div class="row-fluid login-wrapper">
        <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/index.html">
            <img class="logo" src="./Detail Admin - Sign in_files/logo-white.png">
        </a>

        <div class="span4 box">
            <div class="content-wrap">
                <h6>Log in</h6>
                <input class="span12" type="text" placeholder="E-mail address">
                <input class="span12" type="password" placeholder="Your password">
                <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signin.html#" class="forgot">Forgot password?</a>
                <div class="remember">
                    <input id="remember-me" type="checkbox">
                    <label for="remember-me">Remember me</label>
                </div>
                <a class="btn-glow primary login" href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/index.html">Log in</a>
            </div>
        </div>

        <div class="span4 no-account">
            <p>Don't have an account?</p>
            <a href="http://demo2.cssmoban.com/cssthemes6/11.19ZF3/signup.html">Sign up</a>
        </div>
    </div>

    <!-- scripts -->
    <script src="./Detail Admin - Sign in_files/jquery-latest.js.download"></script>
    <script src="./Detail Admin - Sign in_files/bootstrap.min.js.download"></script>
    <script src="./Detail Admin - Sign in_files/theme.js.download"></script>

    <!-- pre load bg imgs -->
    <script type="text/javascript">
        $(function () {
            // bg switcher
            var $btns = $(".bg-switch .bg");
            $btns.click(function (e) {
                e.preventDefault();
                $btns.removeClass("active");
                $(this).addClass("active");
                var bg = $(this).data("img");

                $("html").css("background-image", "url('img/bgs/" + bg + "')");
            });

        });
    </script>


</body>
</html>